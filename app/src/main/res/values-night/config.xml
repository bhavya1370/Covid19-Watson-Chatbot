<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <!--Watson Assistant service credentials-->
    <!-- REPLACE `ASSISTANT_ID_HERE` with ID of the Assistant to use -->
    <string name="assistant_id" translatable="false" tools:ignore="ExtraTranslation">Your_Assistant_ID</string>

    <!-- REPLACE `ASSISTANT_API_KEY_HERE` with Watson Assistant service API Key-->
    <string name="assistant_apikey" translatable="false" tools:ignore="ExtraTranslation">Your_Assistant_apikey</string>

    <!-- REPLACE the URL with the one provided in the credentials of the service you created-->
    <string name="assistant_url" translatable="false" tools:ignore="ExtraTranslation">Your_Assistant_url</string>

</resources>
